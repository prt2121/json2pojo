<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
        prefHeight="520.0" prefWidth="640.0" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="com.prt2121.pojo.MainController"
        xmlns="http://javafx.com/javafx/2.2">
    <children>
        <AnchorPane prefHeight="520.0" prefWidth="640.0">
            <children>
                <TextArea layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="612.0"
                        promptText="{ &quot;your_json&quot; : &quot;here&quot; }" wrapText="true"
                        fx:id="jsonTextArea"/>
                <TextField layoutX="14.0" layoutY="424.0" prefHeight="28.0" prefWidth="306.0"
                        promptText="package" fx:id="packageNameField"/>
                <TextField layoutX="320.0" layoutY="424.0" prefHeight="28.0" prefWidth="306.0"
                        promptText="class" text="" fx:id="classNameField"/>
                <Button layoutX="230.0" layoutY="468.0" mnemonicParsing="false" prefHeight="28.0"
                        prefWidth="180.0" text="Generate" fx:id="generateButton"/>
            </children>
        </AnchorPane>
    </children>
    <stylesheets>
        <URL value="@json2pojo.css"/>
    </stylesheets>
</VBox>
